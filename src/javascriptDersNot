dizi.pop() // diziden son elemanı çıkararak getir
clear()    // konsol ekranı temizler.
var deger = prompt("ekranda kullanıcıya gözükecek yazı buraya yazılır")   // kullanıcıdan veri girmesini ister ve "deger" değişkenine atar
alert("merhaba") // ekrana popup formatında bir mesaj çıkartır ve merhaba yazar.
dizi.shift() // bastan ilk elemanı diziden çıkartır ve çıkarttığı elemanı getirir.
dizi.push("eleman") // dizinin sonuna "eleman" isimli degiskeni ekler ve dizinin eleman sayısını getirir.
dizi.concat(["eleman1","eleman2","eleman3"]) diziye  sırasıyla "eleman1", "eleman2" ve "eleman3" dizi elemanlarını ekler ve diziyi getirir. Ancak eski dizinin ilk halini değiştirmez.
dizi.short() // diziyi alfabetik olarak sıralar.
dizi.lenght dizinin uzunluğunu yani eleman sayısını getirir.
örnek nesne tanımı;

var person = {
    name : "oğuzhan"
    lastName : "Kocabaş"
    age : 25
    bodyLength : "1.80cm"
}

// kullanırken 

person.name // çıktısı "oğuzhan"


var queryElemanlari = document.querySelectorAll("p.intro1") // paragraf "p" tegine sahip  ve "intro1" className e sahip olanları getir anlamına gelir ve geri dönüş değeri bir html obje tutan bir dizidir.

///////////////////////////////////////
var baslik = document.createElement("h1");                     // yeni bir element oluşturuldu
var node = document.createTextNode("Merhaba Dom Node yapisi")   // içine yazılacak text  node ile oluşturuldu
baslik.appendChild(node)                                        // node ile element ilişkilendirildi
   
var divGenel = document.getElementById("div1")                  // elementin nereye konacağı bilgisi için hazırda var olan div bulundu
var p2 = document.getElementById("p2")                          // o divin içindeki p2 id li kısmın üstüne konması için p2 elementi seçildi.

divGenel.insertBefore(baslik,p2)                                // div1 id li divin içindeki p2 elementinden öncesinde h1 elementi eklendi
                                                                /// ...buraya kadar
////////////////////////////////////////